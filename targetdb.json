{
    "name": "targetdb",
    "type": "redis",
    "memory_size":  30000000000,
    "port": 12000,
    "proxy_policy": "single",
    "sharding": true,
    "shards_count": 1,
    "shards_placement": "sparse",
    "shard_key_regex": [{"regex": ".*\\{(?<tag>.*)\\}.*"}, {"regex": "(?<tag>.*)"}],
    "replication": false,
    "authentication_redis_pass": "redis",
    "module_list": [{
        "module_name":"ReJSON",
        "module_args": "",
        "semantic_version": "2.4.6"
    },
    {
        "module_name": "search",
        "module_args": "",
        "semantic_version": "2.6.9"
    }]
}